			if(getCurentFileName()=='index.cfm' || getCurentFileName()==''){
				$("#a").css("display", "block");
				$("#b").css("display", "block");
			}

			$(".cancel").click(function () {
				$("#a").fadeOut();
				$("#b").fadeOut();
			});

		});
		
		function getCurentFileName(){
			var pagePathName= window.location.pathname;
			return pagePathName.substring(pagePathName.lastIndexOf("/") + 1);
		}

