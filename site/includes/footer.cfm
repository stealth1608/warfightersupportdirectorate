	<script type="text/javascript">
		var speed1 = Number('<cfoutput>#qUserStory.speed1#</cfoutput>')*1000;
		var speed2 = Number('<cfoutput>#qUserStory.speed2#</cfoutput>')*1000;
		var speed3 = Number('<cfoutput>#qUserStory.speed3#</cfoutput>')*1000;
		var speed4 = Number('<cfoutput>#qUserStory.speed4#</cfoutput>')*1000;
		var redirectURL = '<cfoutput>#redirectURL#</cfoutput>';
		
		jQuery(document).ready(function( $ ) {
        $('.counter1').counterUp({
            delay: 100,
            time: speed1
        });
        $('.counter2').counterUp({
            delay: 100,
            time: speed2
        });
        $('.counter3').counterUp({
            delay: 100,
            time: speed3
        });
        $('.counter4').counterUp({
            delay: 100,
            time: speed4
        });
    });	
    
		(function ($) {
			if(redirectURL!='index.cfm')
			{
				"use strict";
				Royal_Preloader.config({
					mode: 'text', // 'number', "text" or "logo"
					text: 'Warfighter Support Directorate',
					timeout: 4,
					showInfo: true,
					opacity: 1,
					background: ['#292929']
				});	
			}
			else
			{
				"use strict";
				Royal_Preloader.config({
					mode: 'text', // 'number', "text" or "logo"
					text: '',
					timeout: 0,
					showInfo: true,
					opacity: 0,
					background: ['#292929']
				});
			}
			
			
			
			
		})(jQuery);

		function sendEmail()
		{
			var mail = document.createElement("a");

			mail.target = "_blank";
			mail.href = "mailto:DLL-WSD-FEEDBACK@usace.army.mil?subject="+$('#subject').val()+"&body=Name: "+$('#name').val()+" %0D%0A%0D%0A"+$('#message').val();
			mail.click();
		}
	</script>
